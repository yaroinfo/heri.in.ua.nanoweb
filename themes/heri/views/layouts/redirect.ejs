<html lang="<%= $lang %>" data-bs-theme="dark" data-gtag-id="<%= global['analytics']?.['gtag']?.['id'] ?? '' %>">
    <head>
        <%- include('/_/tracking/gtag', { target: 'head' }) %>

        <meta name="viewport" content="width=device-width,minimum-scale=1.0,initial-scale=1,user-scalable=yes"/>
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,300;0,500;0,700;1,300;1,400;1,700&display=swap" rel="stylesheet">
        <link rel="stylesheet" href="/css/index.css?v=<%= version %>">
        <link rel="stylesheet" href="/css/font-icons.css?v=<%= version %>">

        <meta http-equiv="refresh" content="0; url=<%= $redirect %>">
    </head>
    <body style="height: 100vh; background-color: #fff; color: #FAFA09; font-size: 64px; display: flex; justify-content: center; align-items: center;">
        <img src="/img/logo-<%= $lang %>.png" alt="<%= l('logoTitle', $l) %>" style="max-height: 10rem">
    </body>
</html>