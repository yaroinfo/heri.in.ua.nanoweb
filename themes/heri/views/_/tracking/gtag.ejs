<% if ($rendering && global['analytics'] && global['analytics']['gtag'] && global['analytics']['gtag']['id']) { %>
    <% if (target === 'head') { %>
    <!-- Google Tag Manager -->

    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=<%= global.analytics.gtag.id %>"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', '<%= global.analytics.gtag.id %>');
    </script>
    <!-- End Google Tag Manager -->
    <% } else if (target === 'body') { %>
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=<%= global.analytics.gtag.id %>"
        height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
        <!-- End Google Tag Manager (noscript) -->
    <% } %>
<% } %>